{
  "name": "electron-react-quick-start",
  "productName": "Fake Google Docs",
  "version": "1.0.0",
  "description": "A minimal Electron application with React and Express",
  "main": "main.js",
  "scripts": {
    "dev": "concurrently -k \"npm run devWebpack\" \"npm run devApp\" \"npm run devServer\"",
    "devWebpack": "webpack-dev-server --content-base build/",
    "devApp": "electron main.js --dev",
    "devServer": "nodemon --watch backend backend/server.js",
    "webpack": "webpack",
    "prodWebpack": "webpack -p",
    "start": "electron main.js",
    "server": "node backend/server.js",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Fake Google Docs\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bvi1994/electron-react-quick-start.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.0",
    "electron": "^1.7.9",
    "electron-packager": "^9.1.0",
    "eslint": "^3.0.1",
    "eslint-plugin-react": "^5.2.2",
    "loglevel": "^1.4.1",
    "nodemon": "^1.11.0",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^2.6.0"
  },
  "dependencies": {
    "axios": "^0.17.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cookie-session": "^2.0.0-beta.3",
    "draft-js": "^0.10.4",
    "ejs": "^2.5.7",
    "express": "^4.15.3",
    "express-handlebars": "^3.0.0",
    "fs": "0.0.1-security",
    "http": "0.0.0",
    "immutable": "^3.8.2",
    "local": "^0.3.3",
    "material-ui": "^0.19.4",
    "mongoose": "^4.12.5",
    "mousetrap": "^1.6.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "prop-types": "^15.6.0",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.5",
    "react-color": "^2.13.8",
    "react-dom": "^15.6.1",
    "react-fontawesome": "^1.6.1",
    "react-router-dom": "^4.2.2",
    "react-tap-event-plugin": "^3.0.2",
    "reflexbox": "^3.0.0-0",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4"
  },
  "bugs": {
    "url": "https://github.com/bvi1994/electron-react-quick-start/issues"
  },
  "homepage": "https://github.com/bvi1994/electron-react-quick-start#readme"
}
